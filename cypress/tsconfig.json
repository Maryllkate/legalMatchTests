{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es5", "dom"],
      "types": ["cypress", "node"],
      "module": "commonjs",
      "moduleResolution": "node",
      "preserveValueImports": false,
      "resolveJsonModule": true,
      "outDir": "./dist",
      "esModuleInterop": true,
      "skipLibCheck": true,
    },
    "include": ["**/*.ts", "cypress/**/*.ts"],
    "exclude": ["cypress.config.ts", "node_modules"]
}
